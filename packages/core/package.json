{
  "name": "es-roller",
  "version": "0.0.1-alpha.1",
  "description": "es rollupjs library builder",
  "author": "Eric Sotomayor <elpasocolorartist@gmail.com>",
  "license": "ISC",
  "main": "./dist/es-roller.cjs.js",
  "module": "./dist/es-roller.es.js",
  "typings": "./dist/es-roller.d.js",
  "files": [
    "./dist/**/*",
    "dist/**/*.js",
    "dist/*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/es-roller.cjs.js",
      "import": "./dist/es-roller.es.js",
      "types": "./dist/es-roller.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -cw"
  },
  "dependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-arrow-functions": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/node": "^18.7.9",
    "rollup": "^2.78.1",
    "rollup-plugin-add-shebang": "^0.3.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-externals": "^4.1.1",
    "rollup-plugin-ts": "^3.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esoto76/es-roller.git"
  },
  "bugs": {
    "url": "https://github.com/esoto76/es-roller/issues"
  },
  "homepage": "https://github.com/esoto76/es-roller/tree/main/packages/core#readme"
}